  "main": "build/index.js",
  "name": "@pzzld/template-rs-cloudflare",
  "readme": "README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FL03/template-rs-cloudflare.git"
  },
  "version": "0.0.0",
  "scripts": {
    "setup": "npm run cargo:setup",
    "build": "npm run worker:build",
    "deploy": "npm run cf:deploy",
    "dev": "npm run cf:dev",
    "cargo:setup": "cargo install cargo-binstall && cargo binstall wasm-pack worker-build -y",
    "cf:build": "npx wrangler build",
    "cf:deploy": "npx wrangler deploy",
    "cf:dev": "npx wrangler dev",
    "wasm-pack:build": "wasm-pack build --target web --release -d .artifacts/wasm/js --features app",
    "worker:build": "worker-build --release -d dist --features app"
  }
}